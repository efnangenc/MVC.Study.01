
@{
    var users = ViewData["Users"] as List<User>;
}

<table border="1">
    <tr>
        <th>Id</th>
        <th>Ad</th>
        <th>Email</th>
        <th>--</th>
    </tr>
    @foreach (var user in users)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Name</td>
            <td>@user.Email</td>
            <td></td>
        </tr>
    }
</table>